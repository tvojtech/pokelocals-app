{
  "name": "ptcg-pairings",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "prettier --loglevel warn --write ./src/**/* && next lint --fix",
    "ts-check": "tsc --noEmit",
    "edge:g:interface": "npx @edgedb/generate interfaces",
    "edge:g:query": "npx @edgedb/generate queries && npm run lint-fix",
    "prepare": "test -n \"$CI\" || husky"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,md}": "prettier --write"
  },
  "dependencies": {
    "@auth/edgedb-adapter": "^1.7.4",
    "@netlify/blobs": "^8.1.0",
    "@netlify/functions": "^2.8.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toggle": "^1.1.1",
    "@radix-ui/react-toggle-group": "^1.1.1",
    "@rollbar/react": "^0.12.0-beta",
    "@uidotdev/usehooks": "^2.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "edgedb": "^1.5.12",
    "fast-xml-parser": "^4.5.0",
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.468.0",
    "next": "^15.2.0-canary.62",
    "next-auth": "^5.0.0-beta.25",
    "nextjs-toploader": "^3.7.15",
    "qrcode.react": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollbar": "^2.26.4",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.31.3",
    "@edgedb/generate": "^0.5.6",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20"
  }
